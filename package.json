{
  "version": "0.2.5",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "lazerpay-node-sdk",
  "author": "Njoku Emmanuel",
  "module": "dist/lazerpay-node-sdk.esm.js",
  "size-limit": [
    {
      "path": "dist/lazerpay-node-sdk.cjs.production.min.js",
      "limit": "10.5 KB"
    },
    {
      "path": "dist/lazerpay-node-sdk.esm.js",
      "limit": "10.5 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.5",
    "husky": "^7.0.2",
    "size-limit": "^5.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "axios": "^0.22.0",
    "dotenv": "^16.0.0"
  }
}
